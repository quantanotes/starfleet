apiVersion: v1
kind: Config
current-context: my-context
clusters:
  - name: my-cluster
    cluster:
      server: https://your-kubernetes-api-server
      certificate-authority-data: BASE64ENCODEDCA
users:
  - name: admin
    user:
      client-certificate-data: BASE64ENCODEDCERT
      client-key-data: BASE64ENCODEDKEY
contexts:
  - name: my-context
    context:
      cluster: my-cluster
      user: my-user
